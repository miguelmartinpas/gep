{
  "extends": "airbnb/base",
  "globals": {
    "angular": true,
    "inject": true,
    "jest": true,
    "REGISTRY_URI": true,
    "IDENTITY_URI": true
  },
  "env": {
    "browser": true,
    "es6": true,
    "jasmine": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "import/extensions": 0,
    "react/prop-types": "off",
    "no-undef": "off",
    "no-console": "off",
    "object-curly-newline": ["error", { "multiline": true, "minProperties": 1 }],
    "comma-dangle": ["error", {
        "arrays": "never",
        "objects": "never",
        "imports": "never",
        "exports": "never",
        "functions": "ignore"
    }]
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 7
  }
}
